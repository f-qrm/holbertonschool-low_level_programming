#include "main.h"
/**
 * print_square - prints a square followed by a new line
 *@size: show size of #
 */
void print_square(int size)
{
	int ver;
	int hor;

	if (size > 0)
	{
		for (ver = 0; ver < size; ver++)
		{
			for (hori = 0; hori < (size - 1); hori++)
				_putchar('#');
			_putchar('#');
			_putchar('\n');
		}
	}
	else
	{
		_putchar('\n');
	}
}
